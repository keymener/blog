<script>
    $(document).ready(function () {
        //hide the alert diV
        $("#jsAlert").hide();

        //on submit form
        $("form").submit(function (event) {

            //kill the submit form
            event.preventDefault();

            // ajax call with form inputs
            $.post(
                "/blog/sendmail",
                {
                    name: $("#name").val(),
                    email: $("#email").val(),
                    message: $("#message").val(),
                    token: $('#token').val(),
                    "g-recaptcha-response": $('g-recaptcha').val()

                },
                jsonReturn,
            );

            //show alert on warning status
            function jsonReturn(json) {

                console.log(json);

                //
                // if (json.status == "warning") {
                //     $("#jsAlert").show().addClass('alert-warning').text(json.message);
                // }
            }


        });



    })

</script>
